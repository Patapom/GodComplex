Cell[CellGroupData[{

Cell["Converting AO into a cone aperture + ZH", "Subchapter",
 CellChangeTimes->{{3.7807454673310156`*^9, 3.7807454822188673`*^9}}],

Cell[TextData[{
 "The idea here is to use the AO value (i.e. a ratio of the visibility sphere \
we consider as unoccluded) into a set of ZH coefficients that will represent \
an occlusion cone.\nFrom Sloan et al. (\[OpenCurlyDoubleQuote]Stupid SH \
Tricks\[CloseCurlyDoubleQuote]), they give the ZH coefficients for a smooth \
cone (Appendix A3) as, for the first 2 bands:\n\n\t\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["Y", "0"], "(", "\[Alpha]", ")"}], "=", " ", 
    FractionBox[
     RowBox[{
      RowBox[{"(", 
       RowBox[{
        SuperscriptBox["\[Alpha]", "3"], " ", "+", 
        RowBox[{"6", "\[Alpha]"}], "-", 
        RowBox[{"12", 
         RowBox[{"sin", "(", "\[Alpha]", ")"}]}], "+", 
        RowBox[{"6", 
         RowBox[{"cos", "(", "\[Alpha]", ")"}], "\[Alpha]"}]}], ")"}], 
      SqrtBox["\[Pi]"]}], 
     SuperscriptBox["\[Alpha]", "3"]]}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "\n\t\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["Y", "1"], "(", "\[Alpha]", ")"}], "=", " ", 
    RowBox[{
     FractionBox["1", "4"], 
     FractionBox[
      RowBox[{
       RowBox[{"(", 
        RowBox[{
         SuperscriptBox["\[Alpha]", "3"], " ", "-", 
         RowBox[{"3", 
          RowBox[{"cos", "(", "\[Alpha]", ")"}], 
          RowBox[{"sin", "(", "\[Alpha]", ")"}]}], "+", 
         RowBox[{"3", 
          SuperscriptBox[
           RowBox[{"cos", "(", "\[Alpha]", ")"}], "2"], "\[Alpha]"}]}], ")"}], 
       SqrtBox[
        RowBox[{"3", "\[Pi]"}]]}], 
      SuperscriptBox["\[Alpha]", "3"]]}]}], TraditionalForm]]]
}], "Text",
 CellChangeTimes->{{3.7807454860350857`*^9, 3.7807457939656982`*^9}}],

Cell[CellGroupData[{

Cell[BoxData[{
 RowBox[{
  RowBox[{"Y0", "[", "\[Alpha]_", "]"}], ":=", 
  FractionBox[
   RowBox[{
    RowBox[{"(", 
     RowBox[{
      SuperscriptBox["\[Alpha]", "3"], " ", "+", 
      RowBox[{"6", "\[Alpha]"}], "-", 
      RowBox[{"12", 
       RowBox[{"Sin", "[", "\[Alpha]", "]"}]}], "+", 
      RowBox[{"6", 
       RowBox[{"Cos", "[", "\[Alpha]", "]"}], "\[Alpha]"}]}], ")"}], 
    SqrtBox["\[Pi]"]}], 
   SuperscriptBox["\[Alpha]", "3"]]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"Y1", "[", "\[Alpha]_", "]"}], ":=", 
  RowBox[{
   FractionBox["1", "4"], 
   FractionBox[
    RowBox[{
     RowBox[{"(", 
      RowBox[{
       SuperscriptBox["\[Alpha]", "3"], " ", "-", 
       RowBox[{"3", 
        RowBox[{"Cos", "[", "\[Alpha]", "]"}], 
        RowBox[{"Sin", "[", "\[Alpha]", "]"}]}], "+", 
       RowBox[{"3", 
        SuperscriptBox[
         RowBox[{"Cos", "[", "\[Alpha]", "]"}], "2"], "\[Alpha]"}]}], ")"}], 
     SqrtBox[
      RowBox[{"3", "\[Pi]"}]]}], 
    SuperscriptBox["\[Alpha]", "3"]]}]}], "\[IndentingNewLine]", 
 RowBox[{"Plot", "[", 
  RowBox[{
   RowBox[{"{", 
    RowBox[{
     RowBox[{"Y0", "[", "\[Alpha]", "]"}], ",", 
     RowBox[{"Y1", "[", "\[Alpha]", "]"}]}], "}"}], ",", 
   RowBox[{"{", 
    RowBox[{"\[Alpha]", ",", "0.01", ",", "\[Pi]"}], "}"}]}], "]"}]}], "Input",
 CellChangeTimes->{{3.7807458998187523`*^9, 3.780745983598544*^9}, {
