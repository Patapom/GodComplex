Cell[TextData[{
 "We know that for ZH (i.e. m=0) we have:\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["K", "l"], "=", 
    SqrtBox[
     FractionBox[
      RowBox[{
       RowBox[{"2", "l"}], "+", "1"}], 
      RowBox[{"4", "\[Pi]"}]]]}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "\nSo:\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["K", "0"], "=", 
    SqrtBox[
     FractionBox["1", 
      RowBox[{"4", "\[Pi]"}]]]}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "\tand \t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["K", "1"], "=", 
    SqrtBox[
     FractionBox["3", 
      RowBox[{"4", "\[Pi]"}]]]}], TraditionalForm]]]
}], "Text",
 CellChangeTimes->{{3.7807463064330096`*^9, 3.780746417186344*^9}}],

