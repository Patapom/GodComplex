          RowBox[{"\[Pi]", "*", 
           SuperscriptBox[
            RowBox[{"(", 
             RowBox[{
              RowBox[{
               SuperscriptBox["\[Mu]", "2"], "*", 
               RowBox[{"(", 
                RowBox[{
                 SuperscriptBox["\[Alpha]", "2"], "-", "1"}], ")"}]}], "+", 
              "1"}], ")"}], "2"]}]], "*", "\[Mu]"}],
        TraditionalForm], ";", "\[IndentingNewLine]", 
       RowBox[{"2", "*", "\[Pi]", "*", 
        FormBox[
         RowBox[{
          FractionBox[
           SuperscriptBox["\[Alpha]", "2"], 
           RowBox[{"\[Pi]", "*", 
            SuperscriptBox[
             RowBox[{"(", 
              RowBox[{
               RowBox[{
                SuperscriptBox["\[Mu]", "2"], "*", 
                RowBox[{"(", 
                 RowBox[{
                  SuperscriptBox["\[Alpha]", "2"], "-", "1"}], ")"}]}], "+", 
