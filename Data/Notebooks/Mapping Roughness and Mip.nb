Cell[CellGroupData[{

Cell["Subdivision lin\[EAcute]aire de l\[CloseCurlyQuote]angle solide", \
"Subchapter",
 CellChangeTimes->{{3.7661685236469088`*^9, 3.766168532367408*^9}}],

Cell[TextData[{
 "Imaginons plut\[OHat]t qu\[CloseCurlyQuote]on subdivise \
l\[CloseCurlyQuote]angle solide couvert par la roughness maximale \[Alpha]=1 \
et qui donne:\n\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[CapitalOmega]", "(", "C", ")"}], "=", 
    RowBox[{"2", 
     RowBox[{"\[Pi]", "(", 
      RowBox[{"1", "-", 
       SqrtBox[
        RowBox[{"1", "-", "C"}]]}], ")"}]}]}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "\n\nOn a donc chaque mip qui va couvrir un angle solide toujours un peu \
plus large :\n\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[CapitalOmega]", "(", 
     RowBox[{"mip", ",", "C"}], ")"}], "=", 
    RowBox[{
     FractionBox["mip", "N"], " ", "2", 
     RowBox[{"\[Pi]", "(", 
      RowBox[{"1", "-", 
       SqrtBox[
        RowBox[{"1", "-", "C"}]]}], ")"}]}]}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "\n\nOn cherche donc la roughness correspondant \[AGrave] cet angle solide :\
\n\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[CapitalOmega]", "(", 
     RowBox[{"\[Alpha]", ",", "C"}], ")"}], "=", 
    RowBox[{"2", 
     RowBox[{"\[Pi]", "[", 
      RowBox[{"1", "-", 
       SqrtBox[
        FractionBox[
         RowBox[{"1", "-", "C"}], 
         RowBox[{"1", "+", 
          RowBox[{"C", "(", 
           RowBox[{
            SuperscriptBox["\[Alpha]", "2"], "-", "1"}], ")"}]}]]]}], 
      "]"}]}]}], TraditionalForm]],
  FormatType->"TraditionalForm"],
 "\n\nSi on pose l\[CloseCurlyQuote]\[EAcute]galit\[EAcute] \
\[CapitalOmega](mip,C) = \[CapitalOmega](\[Alpha],C) alors :\n\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     FractionBox["mip", "N"], " ", "2", 
     RowBox[{"\[Pi]", "(", 
      RowBox[{"1", "-", 
       SqrtBox[
        RowBox[{"1", "-", "C"}]]}], ")"}]}], "=", 
    RowBox[{"2", 
     RowBox[{"\[Pi]", "[", 
      RowBox[{"1", "-", 
       SqrtBox[
        FractionBox[
         RowBox[{"1", "-", "C"}], 
         RowBox[{"1", "+", 
          RowBox[{"C", "(", 
           RowBox[{
            SuperscriptBox["\[Alpha]", "2"], "-", "1"}], ")"}]}]]]}], 
      "]"}]}]}], TraditionalForm]]],
 "\n\nSoit : \n\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["\[Alpha]", "2"], "=", 
    RowBox[{
     FractionBox[
      RowBox[{"1", "-", "C"}], "C"], "[", 
     RowBox[{
      FractionBox["1", 
       SuperscriptBox[
        RowBox[{"(", 
         RowBox[{"1", "-", 
          RowBox[{
           FractionBox["mip", "N"], 
           RowBox[{"(", 
            RowBox[{"1", "-", 
             SqrtBox[
              RowBox[{"1", "-", "C"}]]}], ")"}]}]}], ")"}], "2"]], "-", "1"}],
      "]"}]}], TraditionalForm]],
  FormatType->"TraditionalForm"]
}], "Text",
 CellChangeTimes->{{3.7661685344225254`*^9, 3.7661686184073286`*^9}, {
  3.766168672072398*^9, 3.7661687974465694`*^9}, {3.766168832300563*^9, 
  3.766168884124527*^9}, {3.7661689163013673`*^9, 3.766168981278084*^9}, {
  3.766169339291561*^9, 3.766169386568265*^9}}],

