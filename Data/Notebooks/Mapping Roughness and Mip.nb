     RowBox[{"mip", ",", "c"}], "]"}], "*", 
    SuperscriptBox["2", 
     RowBox[{"mip", "-", "1"}]]}]]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"s", "[", 
   RowBox[{"mip_", ",", "c_"}], "]"}], ":=", 
  FractionBox[
   RowBox[{"3", "*", 
    SuperscriptBox["256", "2"]}], 
   RowBox[{"2", "\[Pi]", " ", "*", " ", 
    RowBox[{"p", "[", 
     RowBox[{"mip", ",", "c"}], "]"}], "*", 
    SuperscriptBox["2", "mip"]}]]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"N", "[", 
   RowBox[{"mu", "[", "7", "]"}], "]"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"N", "[", 
   RowBox[{"p", "[", 
    RowBox[{"5", ",", 
     FractionBox["5", "9"]}], "]"}], "]"}], ";"}]}], "Input",
